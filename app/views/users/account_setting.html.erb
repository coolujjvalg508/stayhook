

<body>
<div class="dashbord-sec"> 

  <!-- Header Start -->
  <header class="cp_header">
    
  </header>
  <div class="clearfix"></div>
  
  <!-- dashboard-secn -->

  <div class="lower-section">
    <div class="container">
      <div class="row">
        <div class="col-md-3">
          <div class="left-muns">
            <div class="user-pics"> <span class="user-img"><img src="/assets/dashboard-user.jpg" width="120px" class="img-circle img-responsive" alt=""></span> <b>Shinendra Kumar</b> </div>
            <ul>
              <li><a href="booking-history.html">Booking</a></li>
              <li><a href="profile.html" class="active">Profile</a></li>
              <li><a href="#">Sign Out</a></li>
            </ul>
          </div>
        </div>
        <div class="col-md-9">
          <div class="right-dashboard">
            <ul class="edit-secs">
              <li class="active"><a data-toggle="tab" href="#edit-prof">Edit Profile</a></li>
              <li><a data-toggle="tab" href="#change-pass">Change Password</a></li>
            </ul>
            <div class="tab-content">
              <div id="edit-prof" class="tab-pane fade in active">
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group">
                      <label>First Name </label>
                      <input type="text" placeholder="First Name" class="form-control">
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <label>Last Name </label>
                      <input type="text" placeholder="Last Name " class="form-control">
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <label>Email</label>
                      <input type="text" class="form-control" placeholder="someone@example.com">
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group gender-">
                      <label>I’am</label>
                      <select>
                        <option>Choose Gender</option>
                        <option>Male</option>
                        <option>Female</option>
                      </select>
                    </div>
                  </div>
                  <div class="col-md-12">
                    <div class="form-group update-butn">
                      <button type="button">UPDATE</button>
                    </div>
                  </div>
                </div>
              </div>
              <div id="change-pass" class="tab-pane fade">
                <div class="row">
                  <div class="col-sm-6">
                    <div class="col-md-12">
                      <div class="form-group">
                        <label>Current Password </label>
                        <input type="text" placeholder="Current Password" class="form-control">
                      </div>
                    </div>
                    <div class="col-md-12">
                      <div class="form-group">
                        <label>Last Name </label>
                        <input type="password" placeholder="New Password" class="form-control">
                        <ul class="info-text">
                          <li>&gt;	Password should have atleast 8 characters.</li>
                          <li>&gt;	Password should have atleast 1 special character..</li>
                        </ul>
                      </div>
                    </div>
                    <div class="col-md-12">
                      <div class="form-group update-butn text-center">
                        <button type="button">UPDATE PASSWORD</button>
                      </div>
                    </div>
                  </div>
                  <div class="col-sm-6">
                    <div class="text-center send-opt">
                      <h5>Don’t Know Your Password ?</h5>
                      <a href="#" class="esn-ot">Send OTP</a>
                      <p>We have sent the OTP to 987123456.<br>
                        You can enter the OTP in the left to set your new password.</p>
                      <a href="#" class="rese-ot">Resend OTP</a> </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
